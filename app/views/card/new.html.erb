<% provide(:title, 'Create card') %>
<h1>Create card</h1>

<div>
	<%= form_for(@card, url: new_card_path) do |f| %>
		<%= f.label :name %>
		<%= f.text_field :name %>

		<%= f.label :discription %>
		<%= f.text_area :discription %>

		<%= f.label :clumnId %>
		<%= f.text_field :column_id %>

		<%= f.label :deadline %>
		<%= f.date_select :deadline %>

		<%= f.submit "Submit", disable_with: 'Submiting...' %>
	<% end %>
</div>